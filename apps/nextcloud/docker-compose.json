{
    "services": [
        {
            "name": "nextcloud",
            "image": "lscr.io/linuxserver/nextcloud:31.0.6",
            "isMain": true,
            "internalPort": 443,
            "environment": {
                "PUID": 1000,
                "PGID": 1000,
                "TZ": "${TZ}"
            },
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/data/config",
                    "containerPath": "/config"
                },
                {
                    "hostPath": "${APP_DATA_DIR}/data/data",
                    "containerPath": "/data"
                }
            ]
        }
    ]
}