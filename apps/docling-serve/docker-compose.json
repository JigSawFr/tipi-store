{
    "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
    "schemaVersion": 2,
    "services": [
        {
            "name": "docling-serve",
            "image": "ghcr.io/docling-project/docling-serve-cpu:v1.11.0",
            "internalPort": 5001,
            "isMain": true,
            "environment": [
                {
                    "key": "DOCLING_SERVE_ENABLE_UI",
                    "value": "${DOCLINGSERVE_ENABLE_UI}"
                },
                {
                    "key": "TZ",
                    "value": "${TZ}"
                }
            ],
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/data",
                    "containerPath": "/app/data"
                }
            ]
        }
    ]
}