{
  "services": [
    {
      "name": "homebox",
      "image": "ghcr.io/sysadminsmedia/homebox:0.20.0-rootless",
      "isMain": true,
      "internalPort": 7745,
      "user": "1000:1000",
      "environment": {
        "HBOX_LOG_LEVEL": "info",
        "HBOX_LOG_FORMAT": "text",
        "HBOX_WEB_MAX_UPLOAD_SIZE": "10",
        "HBOX_OPTIONS_ALLOW_REGISTRATION": "${HBOX_OPTIONS_ALLOW_REGISTRATION-false}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/data/"
        }
      ]
    }
  ]
}