{
  "services": [
    {
      "name": "sonarr",
      "image": "ghcr.io/11notes/sonarr:4.0.14",
      "isMain": true,
      "internalPort": "8989",
      "environment": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "${TZ}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/sonarr/etc"
        },
        {
          "hostPath": "${ROOT_FOLDER_HOST}/media",
          "containerPath": "/media"
        }
      ]
    }
  ]
}