{
  "$schema": null,
  "schemaVersion": 2,
  "services": [
    {
      "image": "ghcr.io/11notes/sonarr:4.0.16",
      "name": "sonarr",
      "isMain": true,
      "internalPort": "8989",
      "environment": [
        {
          "key": "PUID",
          "value": "1000"
        },
        {
          "key": "PGID",
          "value": "1000"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/sonarr/etc"
        },
        {
          "hostPath": "${ROOT_FOLDER_HOST}/media",
          "containerPath": "/media"
        }
      ]
    }
  ]
}
