{
  "services": [
    {
      "name": "huntarr",
      "image": "ghcr.io/plexguide/huntarr:8.1.6",
      "isMain": true,
      "internalPort": "9705",
      "environment": {
        "TZ": "${TZ}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/config"
        }
      ]
    }
  ]
}