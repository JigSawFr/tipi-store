{
  "services": [
    {
      "name": "huntarr",
      "image": "ghcr.io/plexguide/huntarr:8.2.10",
      "isMain": true,
      "internalPort": "9705",
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/config"
        }
      ]
    }
  ],
  "schemaVersion": 2
}