{
  "services": [
    {
      "name": "tautulli",
      "image": "ghcr.io/linuxserver/tautulli:2021.12.16",
      "isMain": true,
      "internalPort": 8181,
      "environment": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "${TZ}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/config"
        }
      ]
    }
  ]
}