{
  "services": [
    {
      "name": "swaparr",
      "image": "ghcr.io/thijmengthn/swaparr:0.10.0",
      "isMain": true,
      "internalPort": "8080",
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        },
        {
          "key": "BASEURL",
          "value": "${SWAPARR_BASEURL}"
        },
        {
          "key": "APIKEY",
          "value": "${SWAPARR_APIKEY}"
        },
        {
          "key": "PLATFORM",
          "value": "${SWAPARR_PLATFORM-sonarr}"
        },
        {
          "key": "MAX_STRIKES",
          "value": "${SWAPARR_MAX_STRIKES-3}"
        },
        {
          "key": "SCAN_INTERVAL",
          "value": "${SWAPARR_SCAN_INTERVAL-10m}"
        },
        {
          "key": "MAX_DOWNLOAD_TIME",
          "value": "${SWAPARR_MAX_DOWNLOAD_TIME-2h}"
        },
        {
          "key": "IGNORE_ABOVE_SIZE",
          "value": "${SWAPARR_IGNORE_ABOVE_SIZE-25GB}"
        },
        {
          "key": "REMOVE_FROM_CLIENT",
          "value": "${SWAPARR_REMOVE_FROM_CLIENT-true}"
        },
        {
          "key": "DRY_RUN",
          "value": "${SWAPARR_DRY_RUN-true}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/app/data"
        }
      ]
    }
  ],
  "schemaVersion": 2
}