{
    "services": [
        {
            "name": "portall",
            "image": "need4swede/portall:2.0.4",
            "isMain": true,
            "internalPort": 8080,
            "environment": {
                "SECRET_KEY": "${PORTALL_SECRET_KEY}",
                "DOCKER_HOST": "${PORTALL_DOCKER_HOST:-tcp://socket-proxy:2375}",
                "DOCKER_ENABLED": "${PORTALL_DOCKER_ENABLED:-true}",
                "HOST_IP": "${PORTALL_HOST_IP:-}",
                "TZ": "${TZ}",
                "USER_ID": "${UID:-1000}",
                "GROUP_ID": "${GID:-1000}"
            },
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/data",
                    "containerPath": "/app/instance"
                }
            ],
            "healthCheck": {
                "test": "curl -f http://localhost:8080 || exit 1",
                "interval": "30s",
                "timeout": "10s",
                "retries": 3,
                "startPeriod": "60s"
            }
        }
    ]
}