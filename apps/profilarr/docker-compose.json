{
  "services": [
    {
      "name": "profilarr",
      "image": "santiagosayshey/profilarr:v1.1.3",
      "isMain": true,
      "internalPort": "6868",
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/config"
        }
      ]
    }
  ],
  "schemaVersion": 2
}