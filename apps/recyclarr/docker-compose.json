{
  "services": [
    {
      "name": "recyclarr",
      "image": "ghcr.io/recyclarr/recyclarr:7.4.1",
      "isMain": true,
      "user": "${TIPI_UID}:${TIPI_GID}",
      "environment": [
        {
          "key": "RECYCLARR_CREATE_CONFIG",
          "value": "${RECYCLARR_CREATE_CONFIG-true}"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/config"
        }
      ]
    }
  ],
  "schemaVersion": 2
}