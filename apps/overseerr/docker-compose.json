{
  "services": [
    {
      "name": "overseerr",
      "image": "ghcr.io/linuxserver/overseerr:2021.12.12",
      "isMain": true,
      "internalPort": "5055",
      "environment": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "${TZ}"
      },
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/config"
        }
      ]
    }
  ]
}