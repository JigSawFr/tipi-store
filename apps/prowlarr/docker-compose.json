{
  "services": [
    {
      "name": "prowlarr",
      "image": "ghcr.io/11notes/prowlarr:2.3.0",
      "isMain": true,
      "internalPort": "9696",
      "environment": [
        {
          "key": "PUID",
          "value": "1000"
        },
        {
          "key": "PGID",
          "value": "1000"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/prowlarr/etc"
        }
      ]
    }
  ],
  "schemaVersion": 2
}