{
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "schemaVersion": 2,
  "services": [
    {
      "image": "ghcr.io/civilblur/mazanoke:v1.1.5",
      "name": "mazanoke",
      "isMain": true,
      "internalPort": 80,
      "environment": [
        {
          "key": "USERNAME",
          "value": "${MAZANOKE_USERNAME}"
        },
        {
          "key": "PASSWORD",
          "value": "${MAZANOKE_PASSWORD}"
        }
      ]
    }
  ]
}
